<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="tests">
  <listeners>
		<listener class-name="listeners.Listener"></listener>
	</listeners>
	
	<test name="Student login">
		<classes>
			<class name="studentTests.StudentLoginTest" />
		</classes>
	</test>
	<test name="Update user">
		<classes>
			<class name="studentTests.UpdateUserTest" />
		</classes>
	</test>
	
	<test name="Enrol into course">
		<classes>
			<class name="studentTests.CourseEnrollmentTest" />
		</classes>
	</test>
	<test name="badges and feedback">
		<classes>
			<class name="studentTests.BadgesFeedbackTest" />
		</classes>
	</test>
	<test name="download certificate">
		<classes>
			<class name="studentTests.DownloadCertificateTest" />
		</classes>
	</test>
</suite> <!-- Suite -->
